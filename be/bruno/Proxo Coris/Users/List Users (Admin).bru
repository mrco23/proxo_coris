meta {
  name: List Users (Admin)
  type: http
  seq: 3
}

get {
  url: {{base_url}}/users
  body: none
  auth: bearer
}

params:query {
  page: 1
  per_page: 10
}

auth:bearer {
  token: {{admin_access_token}}
}

script:post-response {
  if (res.status === 200) {
    bru.setVar("user_id", res.body.data[0].id);
  }
}
